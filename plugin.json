{
    "id": "com.rr250.report-post-plugin",
    "name": "Report Post",
    "description": "Allow to report posts",
    "version": "0.0.1",
    "server": {
      "executables": {
          "linux-amd64": "server/dist/plugin-linux-amd64",
          "darwin-amd64": "server/dist/plugin-darwin-amd64",
          "windows-amd64": "server/dist/plugin-windows-amd64.exe"
      }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "settings": [{
            "key": "ChannelID",
            "display_name": "Channel ID",
            "type": "text",
            "help_text": "Channel ID where Report Post Bot will post the Report Details"
          }
        ]
    }
}
